public class Fraction {
    private int num, denom;
    public Fraction ( int n, int d) {
        num = n;
        denom = d;
        
    }
    public String toString{
        retun "" + num + "/" + denom
    }
    public double getValue(){
        return (double) num/denom; 
    }

